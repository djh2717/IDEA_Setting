<application>
  <component name="DefaultFont">
    <option name="FONT_SIZE" value="22" />
    <option name="FONT_REGULAR_SUB_FAMILY" value="Regular" />
    <option name="FONT_BOLD_SUB_FAMILY" value="Bold" />
    <option name="LINE_SPACING" value="1.5" />
  </component>
  <component name="EditorSettings">
    <option name="languageBreadcrumbsMap">
      <map>
        <entry key="JAVA" value="true" />
        <entry key="XML" value="true" />
      </map>
    </option>
    <option name="USE_SOFT_WRAPS" value="" />
    <option name="SHOW_BREADCRUMBS_ABOVE" value="true" />
    <option name="SHOW_BREADCRUMBS" value="false" />
    <option name="ENABLE_RENDERED_DOC" value="true" />
  </component>
  <component name="JetEditorOptions">
    <option name="donTShowConversionDialog" value="true" />
  </component>
  <component name="TodoConfiguration">
    <pattern useCustomColors="true" pattern="\btodo\b.*">
      <option name="FOREGROUND" value="bf99a6" />
      <option name="FONT_TYPE" value="3" />
      <option name="ERROR_STRIPE_COLOR" value="977ab" />
    </pattern>
    <pattern useCustomColors="true" pattern="\bfixme\b.*">
      <option name="FOREGROUND" value="bf99a6" />
      <option name="FONT_TYPE" value="3" />
      <option name="ERROR_STRIPE_COLOR" value="977ab" />
    </pattern>
    <pattern useCustomColors="true" pattern="\brefactor\b.*">
      <option name="FOREGROUND" value="bf99a6" />
      <option name="FONT_TYPE" value="3" />
      <option name="ERROR_STRIPE_COLOR" value="977ab" />
    </pattern>
    <pattern useCustomColors="true" pattern="\bnotice\b.*">
      <option name="FOREGROUND" value="bf99a6" />
      <option name="FONT_TYPE" value="3" />
      <option name="ERROR_STRIPE_COLOR" value="977ab" />
    </pattern>
    <filter name="ShowTODO">
      <pattern index="0" />
    </filter>
  </component>
</application>